-A INPUT -p $P_ICMP $ICMP_TYPE 7 -j ACCEPT
-A OUTPUT -p $P_ICMP $ICMP_TYPE 6 -j ACCEPT
-A OUTPUT -p $P_ICMP $ICMP_TYPE 4 -j ACCEPT
-A OUTPUT -p $P_ICMP $ICMP_TYPE 8 -j ACCEPT
-A OUTPUT -p $P_ICMP $ICMP_TYPE 7 -j ACCEPT
-A FORWARD -p $P_ICMP $ICMP_TYPE 6 -j ACCEPT
-A FORWARD -p $P_ICMP $ICMP_TYPE 4 -j ACCEPT
-A FORWARD -p $P_ICMP $ICMP_TYPE 8 -j ACCEPT
